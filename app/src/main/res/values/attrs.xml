            <enum name="PartiKodlari" value="5" />
            <enum name="YaziciTasarimList" value="6" />
            <enum name="AmbalajTur" value="7" />
            <enum name="Siparisler" value="8" />
        </attr>
    </declare-styleable>

</resources>
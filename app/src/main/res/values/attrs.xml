        <attr name="history" format="boolean" />
        <attr name="labelText" format="string" />
        <attr name="comboSourceValues" format="string" />
        <attr name="comboSource" format="enum">
            <enum name="ArrayList" value="0" />
            <enum name="Vardiyalar" value="1" />
            <enum name="Departmanlar" value="2" />
            <enum name="KontrolSebebpleri" value="3" />
